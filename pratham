l=[]
for i in range(4):
    n=int(input("enter no."))
    l.append(n)
print(l)
m=[]
for j in range(4):
    a=int(input("enter no."))
    m.append(a)
print(m)
c=l+m
print(c)
for i in range(8):
    for j in range(8):
        if c[i]<c[j]:
            t=c[i]
            c[i]=c[j]
            c[j]=t
print("1st set:",l,"\t2nd set",m,"\tMerging:",c) 
print()
